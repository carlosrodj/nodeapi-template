{
  "name": "template-api",
  "version": "0.0.1",
  "description": "API do aplicativo XYZ",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && babel app -d build && npm run doc-build",
    "clean": "rimraf build && mkdir build",
    "start": "npm run build && node build/index.js",
    "start-dev": "npm run doc && nodemon",
    "prepush": "npm run lint",
    "test": "./node_modules/.bin/mocha ./tests -name '*.spec.js' --recursive --timeout 120000 --require babel-register --exit ",
    "testf": "./node_modules/.bin/mocha --timeout 5000 -b --require babel-register --exit",
    "lint": "./node_modules/.bin/eslint app/**/*.js",
    "test:watch": "nodemon --exec 'npm run test' tests",
    "coverage": "nyc npm test",
    "validate": "npm run test && npm outdated --depth 0",
    "doc": "apidoc -i app/controllers/ -o app/public/doc/",
    "doc-build": "apidoc -i app/controllers/ -o build/public/doc/"
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "nyc": {
    "reporter": [
      "text-summary",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/projeto/repo-api.git"
  },
  "apidoc": {
    "header": {
      "title": "Documentação Chico API",
      "filename": "doc/header.md"
    }
  },
  "keywords": [
    "chico"
  ],
  "author": "Johnys",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/projeto/repo-api/issues"
  },
  "homepage": "https://gitlab.com/projeto/repo-api",
  "dependencies": {
    "apidoc": "^0.17.6",
    "async": "^2.6.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.18.0",
    "bcrypt": "^1.0.0",
    "body-parser": "^1.15.2",
    "cheerio": "^1.0.0-rc.2",
    "cors": "^2.8.1",
    "crawler": "^1.1.2",
    "dotenv": "^5.0.1",
    "express": "^4.14.0",
    "helmet": "^3.1.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.16.4",
    "mail-time": "^1.0.0",
    "method-override": "^2.3.6",
    "moment": "^2.21.0",
    "mongoose": "^5.0.11",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "node-schedule": "^1.3.0",
    "nodemailer": "^4.6.3",
    "uuid": "^3.2.1",
    "yes-https": "0.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.9.0",
    "faker": "^4.1.0",
    "husky": "0.11.9",
    "isparta": "^4.0.0",
    "mocha": "^5.0.4",
    "mocha-lcov-reporter": "^1.3.0",
    "mockgoose": "^7.3.5",
    "nodemon": "^1.11.0",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.4.8",
    "sinon-chai": "^3.0.0",
    "sinon-express-mock": "^2.0.0",
    "sinon-stub-promise": "^4.0.0"
  }
}
